{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./src",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"module": "esNext",
		"moduleResolution": "node",
		"noEmit": false,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": false,
		"outDir": "./dist",
		"paths": {
			"@public/*": [
				"../public/*"
			],
			"@utils/*": [
				"../src/desktop/utils/*"
			]
		},
		"removeComments": true,
		"resolveJsonModule": true,
		"rootDir": "./",
		"rootDirs": [
			"src"
		],
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "es2022",
		"types": [
			"@overwolf/types",
			"node"
		]
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"index.d.ts",
		"src"
	]
}